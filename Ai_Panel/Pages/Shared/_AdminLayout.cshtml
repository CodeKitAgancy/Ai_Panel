<!DOCTYPE html>
<html lang="fa">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"]</title>
	<link href="~/css/bootstrap.min.css" rel="stylesheet" />
	<link href="~/css/bootstrap.rtl.min.css" rel="stylesheet" />
	<link href="~/css/jquery-confirm.min.css" rel="stylesheet" />
	<link href="~/css/jquery-confirm.rtl.css" rel="stylesheet" />
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link href="~/fonts/fontiran.css" rel="stylesheet" />
	<link href="~/css/persian-datepicker.min.css" rel="stylesheet" />
	<link href="~/css/color.css" rel="stylesheet" />
	<link href="~/css/select2.min.css" rel="stylesheet" />
	<link href="~/css/waitme.min.css" rel="stylesheet" />
	<link href="~/css/chat.css" rel="stylesheet" asp-append-version="true" />
	<link rel="stylesheet" href="~/mathlive/mathlive-static.css" />
	<link rel="stylesheet" href="~/mathlive-fonts.css" />
</head>
<body dir="rtl">
	<partial name="_AdminMenu" />
	<div class="container-fluid fix-containerfluied">
		<main role="main" class="pb-3">
			<nav aria-label="breadcrumb">
				<ol class="breadcrumb">
					@await RenderSectionAsync("BreadCrumb", required: false)
				</ol>
			</nav>
			@RenderBody()
		</main>
	</div>
	<partial name="_Modal" />
	<script src="~/js/jquery-3.6.0.min.js"></script>
	<script src="~/js/bootstrap.min.js"></script>
	<script src="~/js/jquery-confirm.min.js"></script>
	<script src="~/lib/jquery/jquery.validate.min.js"></script>
	<script src="~/lib/jquery/jquery.validate.unobtrusive.min.js"></script>
	<script src="~/lib/jquery/jquery.unobtrusive-ajax.min.js"></script>
	<script src="~/js/helper.js" asp-append-version="true"></script>
	<script src="~/js/onlyTypeNumber.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
	<script src="~/js/f02f2d5598.js"></script>
	<script src="~/js/persian-date.min.js"></script>
	<script src="~/js/persian-datepicker.min.js"></script>
	<script src="~/js/select2.full.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	<script src="~/js/waitme.min.js"></script>
	<script src="~/js/modalhandler.js" asp-append-version="true"></script>
	<script src="~/js/book.js"></script>
	<script src="~/js/importexcel.js"></script>
	<script src="~/js/dashboard.js"></script>
	<script src="~/js/sb-admin-2.min.js"></script>
	<script src="~/js/chart.js"></script>
	<script src="~/js/sortable.js"></script>
	<script src="~/js/chat.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
